{
  "version": 2,
  "functions": {
    "django_project/**/*": {
      "excludeFiles": "**/*",
      "includeFiles": "requirements.txt",
      "runtime": "@vercel/python@1.0.0"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/django_project/wsgi.py"
    }
  ]
}
