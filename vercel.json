{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.9"
      }
    },
    {
      "src": "/django_project/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "functions": {
    "api/**/*": {
      "excludeFiles": "**/*"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/django_project/wsgi.py"
    }
  ]
}