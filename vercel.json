
{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",
      "use": "@vercel/python",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/django_project/wsgi.py"
    }
  ]
}
