{
  "version": 2,
  "builds": [
    {
      "src": "build_files.sh",
       "use": "@vercel/python",
      "buildCommand": "python3 -m ensurepip --upgrade && pip install -r requirements.txt",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/django_project/wsgi.py"
    }
  ]
}
